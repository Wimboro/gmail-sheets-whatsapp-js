# Application Configuration
PROCESSOR_USER_ID=email-processor-main
NODE_ENV=development

# Optional: Notification Settings
ENABLE_WHATSAPP_NOTIFICATIONS=true
BATCH_NOTIFICATION_THRESHOLD=5 